{
    "editor.accessibilitySupport": "on",
    "editor.find.seedSearchStringFromSelection": false,
    "editor.fontFamily": "'JetBrainsMono Nerd Font', 'Noto Sans'",
    "editor.lineHeight": 17,
    "editor.minimap.enabled": false,
    "editor.rulers": [
        79,
        119
    ],
    "editor.suggestSelection": "first",
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 119,
    "editor.wrappingIndent": "same",
    "emmet.includeLanguages": {
        "django-html": "html",
        "jinja": "html"
    },
    "explorer.autoReveal": false,
    "files.exclude": {
        "**/**.pyc": true
    },
    "search.exclude": {
        "**/htmlcov": true,
        "**/locale": true
    },
    "search.searchOnType": false,
    "search.showLineNumbers": true,
    "telemetry.enableTelemetry": false,
    "window.menuBarVisibility": "toggle",
    "window.zoomLevel": -2,
    "workbench.activityBar.visible": false,
    "workbench.colorTheme": "Wal",
    "workbench.iconTheme": "quill-icons",
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "none",
    "workbench.statusBar.visible": true,
    "autoHide.autoHidePanel": false,
    "autoHide.hideOnOpen": false,
    "customizeUI.stylesheet": {
        ".decorationsOverviewRuler": "display: none",
        ".monaco-split-view2.vertical > .split-view-container > .split-view-view.visible": "margin-top: 1px",
        "#ms-vsliveshare\\.vsliveshare": "display: none",
        "#status\\.editor\\.eol": "display: none",
        "#status\\.editor\\.indentation": "display: none",
        "#status\\.editor\\.screenReaderMode": "display: none",
        "#status\\.feedback": "display: none",
        "#status\\.scm": "display: none",
        "#kamikillerto\\.vscode-colorize": "display: none",
        "#ryu1kn\\.text-marker": "display: none",
        "#workbench\\.parts\\.editor .editor-actions": "display: none",
        "#workbench\\.parts\\.editor .scroll-decoration": "display: none",
        "#workbench\\.parts\\.editor .split-view-view.visible:before": "display: none",
        "#workbench\\.parts\\.panel": "padding-bottom: 1rem; margin-top: -0.25rem",
        "#workbench\\.parts\\.panel .composite.title": "margin-top: -2px",
        "#workbench\\.parts\\.panel .title-actions": "display: none",
        "#workbench\\.parts\\.panel [title^='Problems']": "display: none",
        "#workbench\\.parts\\.panel [title^='Output']": "display: none",
        "#workbench\\.parts\\.panel [title^='Debug Console']": "display: none",
        "#workbench\\.parts\\.panel [title^='Comments']": "display: none",
        "#workbench\\.parts\\.sidebar": "padding-bottom: 3rem",
        "#workbench\\.parts\\.sidebar .composite.title": "display: none",
        "#workbench\\.parts\\.statusbar": "margin-top: -2px",
        "#workbench\\.view\\.explorer": "margin-top: 35px",
        "body > .monaco-workbench > .monaco-grid-view > .monaco-grid-branch-node > .monaco-split-view2 > .split-view-container > .split-view-view > .monaco-grid-branch-node .monaco-split-view2.horizontal > .split-view-container > .split-view-view.visible": "margin-top: -2px"
    },
    "gistpad.comments.showThread": "never",
    "gistpad.scratchNotes.show": false,
    "gitblame.ignoreWhitespace": true,
    "indentRainbow.colors": [
        "rgba(0, 0, 0, 0.3)",
        "rgba(0, 0, 0, 0)",
        "rgba(0, 0, 0, 0)",
