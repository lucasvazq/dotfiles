        "rgba(0, 0, 0, 0)",
        "rgba(0, 0, 0, 0)",
        "rgba(0, 0, 0, 0)",
        "rgba(0, 0, 0, 0)",
        "rgba(0, 0, 0, 0)",
        "rgba(0, 0, 0, 0)",
        "rgba(0, 0, 0, 0)",
        "rgba(0, 0, 0, 0)",
        "rgba(0, 0, 0, 0)",
        "rgba(0, 0, 0, 0)",
        "rgba(0, 0, 0, 0)",
        "rgba(0, 0, 0, 0)",
        "rgba(0, 0, 0, 0)",
        "rgba(0, 0, 0, 0)",
        "rgba(0, 0, 0, 0)",
        "rgba(0, 0, 0, 0)",
        "rgba(0, 0, 0, 0)",
        "rgba(0, 0, 0, 1)"
    ],
    "indentRainbow.excludedLanguages": [
        "django-html",
        "html",
        "jinja",
        "plaintext"
    ],
    "indentRainbow.errorColor": "rgba(255, 255, 0, 0.15)",
    "indentRainbow.ignoreLinePatterns" : [
        "/^[ \t]*$/g",
    ],
    "python.languageServer": "Pylance",
    "textmarker.autoSelectDistinctiveTextColor": true,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue"
}
