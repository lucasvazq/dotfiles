#!/bin/sh
# In this script, we run startup programs that require timeouts before starts


# restart pulseaudio
pkill pulseaudio &
pulseaudio &

# Run pulseeffects, volumeicon and polybar that depends of pulseaudio
pulseeffects --gapplication-service &
volumeicon &
polybar custom &

# Run picture-of-the-day script thats depends of polybar
picture-of-the-day
